<+><!DOCTYPE html>\r\n<html xmlns:th=\"http://www.thymeleaf.org\" lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n  <title>Index Page</title>\r\n  <style>\r\n    /* Basic Reset */\r\n    * {\r\n        margin: 0;\r\n        padding: 0;\r\n        box-sizing: border-box;\r\n    }\r\n\r\n    /* Background Animation */\r\n    body {\r\n        font-family: 'Arial', sans-serif;\r\n        background: linear-gradient(135deg, #cfcfd0, #5d5a78);\r\n        background-size: 400% 400%;\r\n        animation: gradientBG 10s ease infinite;\r\n        height: 100vh;\r\n        display: flex;\r\n        overflow: hidden;\r\n    }\r\n\r\n    @keyframes gradientBG {\r\n        0% { background-position: 0% 50%; }\r\n        25% { background-position: 50% 100%; }\r\n        50% { background-position: 100% 50%; }\r\n        75% { background-position: 50% 0%; }\r\n        100% { background-position: 0% 50%; }\r\n    }\r\n\r\n    /* Main Layout */\r\n    .main-container {\r\n        display: flex;\r\n        width: 100%;\r\n        height: 100%;\r\n        position: relative;\r\n    }\r\n\r\n    /* Sidebar Styling */\r\n    .sidebar {\r\n        width: 250px;\r\n        background-color: #333;\r\n        color: #fff;\r\n        padding: 20px;\r\n        display: flex;\r\n        flex-direction: column;\r\n        transition: transform 0.3s ease;\r\n    }\r\n\r\n    .sidebar h2 {\r\n        margin-bottom: 20px;\r\n        text-align: center;\r\n    }\r\n\r\n    .user-list,\r\n    .group-list {\r\n        list-style-type: none;\r\n        padding: 0;\r\n    }\r\n\r\n    .user-item,\r\n    .group-item {\r\n        margin-bottom: 15px;\r\n    }\r\n\r\n    .user-item a,\r\n    .group-item a {\r\n        display: block;\r\n        padding: 10px;\r\n        background-color: #444;\r\n        color: #fff;\r\n        text-align: center;\r\n        border-radius: 5px;\r\n        text-decoration: none;\r\n        transition: background-color 0.3s ease-in-out;\r\n    }\r\n\r\n    .user-item a:hover,\r\n    .group-item a:hover {\r\n        background-color: #555;\r\n    }\r\n\r\n    /* Chat Section */\r\n    .chat-section {\r\n        flex-grow: 1;\r\n        padding: 20px;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    #chat-box {\r\n        width: 100%;\r\n        height: 350px;\r\n        border: 1px solid #ccc;\r\n        border-radius: 10px;\r\n        overflow-y: auto;\r\n        padding: 15px;\r\n        background-color: #fff;\r\n        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\r\n        flex-grow: 1;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .message {\r\n        margin-bottom: 10px;\r\n        padding: 10px;\r\n        border-radius: 5px;\r\n        background-color: #f1f1f1;\r\n    }\r\n\r\n    .my-message {\r\n        text-align: right;\r\n        background-color: #d1f1d1;\r\n    }\r\n\r\n    /* Input and Button Styling */\r\n    .input-container {\r\n        display: flex;\r\n        align-items: center;\r\n        border: 1px solid #ccc;\r\n        border-radius: 5px;\r\n        overflow: hidden;\r\n    }\r\n\r\n    #messageInput {\r\n        flex-grow: 1;\r\n        padding: 10px;\r\n        border: none;\r\n        outline: none;\r\n        font-size: 14px;\r\n    }\r\n\r\n    .send-button {\r\n        padding: 10px 20px;\r\n        border: none;\r\n        background-color: #6c63ff;\r\n        color: white;\r\n        cursor: pointer;\r\n        transition: background-color 0.3s ease, transform 0.2s ease;\r\n        font-size: 14px;\r\n    }\r\n\r\n    .send-button:hover {\r\n        background-color: #5348c5;\r\n        transform: scale(1.05);\r\n    }\r\n\r\n    /* Sidebar Toggle Button */\r\n    .toggle-sidebar {\r\n        position: absolute;\r\n        top: 10px;\r\n        left: 10px;\r\n        background: transparent;\r\n        border: none;\r\n        cursor: pointer;\r\n        font-size: 30px;\r\n        color: white;\r\n        display: none; /* Hidden in desktop mode */\r\n    }\r\n\r\n    .toggle-sidebar:focus {\r\n        outline: none;\r\n    }\r\n\r\n    /* Overlay */\r\n    .overlay {\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        background: rgba(0, 0, 0, 0.5);\r\n        z-index: 1;\r\n        display: none;\r\n    }\r\n\r\n    /* Responsive Design */\r\n    @media screen and (max-width: 768px) {\r\n        .sidebar {\r\n            position: absolute;\r\n            left: -250px;\r\n            height: 100%;\r\n            transition: transform 0.3s ease;\r\n            z-index: 2;\r\n        }\r\n\r\n        .sidebar.active {\r\n            left: 0;\r\n        }\r\n\r\n        .toggle-sidebar {\r\n            display: block;\r\n        }\r\n\r\n        #chat-box h2 {\r\n            text-align: center;\r\n            margin-bottom: 10px;\r\n        }\r\n\r\n        .chat-section {\r\n            padding-top: 60px; /* Space for the toggle button */\r\n        }\r\n\r\n        .overlay.active {\r\n            display: block;\r\n        }\r\n    }\r\n  </style>\r\n</head>\r\n<body>\r\n\r\n<!-- Main Layout -->\r\n<div class=\"main-container\">\r\n  <!-- Sidebar (User List & Group Chat) -->\r\n  <div class=\"sidebar\" id=\"sidebar\">\r\n    <h2>Available Users</h2>\r\n    <ul class=\"user-list\">\r\n      <li th:each=\"user : ${users}\" class=\"user-item\">\r\n        <a th:href=\"@{/friend-chat/{username}(username=${user.username})}\">\r\n          Chat with <span th:text=\"${user.name}\"></span>\r\n        </a>\r\n      </li>\r\n    </ul>\r\n\r\n    <h2>Group Chat</h2>\r\n    <ul class=\"group-list\">\r\n      <li class=\"group-item\">\r\n        <a href=\"/talk\" class=\"send-button\">Go to Group Chat</a>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n\r\n  <!-- Chat Section -->\r\n  <div class=\"chat-section\">\r\n    <!-- Toggle Sidebar Button -->\r\n    <button class=\"toggle-sidebar\" id=\"toggleButton\">&#9776;</button>\r\n\r\n    <!-- Overlay -->\r\n    <div class=\"overlay\" id=\"overlay\"></div>\r\n\r\n    <!-- Chat Box -->\r\n    <div id=\"chat-box\">\r\n      <h2>Chat with <span th:text=\"${friend}\"></span></h2>\r\n      <!-- Sample messages for layout -->\r\n      <div class=\"message\">User1: Hello!</div>\r\n      <div class=\"message my-message\">You: Hi!</div>\r\n      <div class=\"message\">User1: How are you?</div>\r\n      <div class=\"message my-message\">You: I'm good, thanks!</div>\r\n      <!-- More messages can be added here -->\r\n    </div>\r\n\r\n    <!-- Message Input and Send Button -->\r\n    <div class=\"input-container\">\r\n      <input type=\"text\" id=\"messageInput\" placeholder=\"Enter your message\" />\r\n      <button class=\"send-button\">Send</button>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n<script>\r\n  const toggleButton = document.getElementById('toggleButton');\r\n  const sidebar = document.getElementById('sidebar');\r\n  const overlay = document.getElementById('overlay');\r\n\r\n  // Toggle the sidebar and show the overlay\r\n  toggleButton.addEventListener('click', function() {\r\n      sidebar.classList.toggle('active');\r\n      overlay.classList.toggle('active');\r\n      toggleButton.style.display = 'none'; // Hide the toggle button when sidebar is active\r\n  });\r\n\r\n  // Hide the sidebar and overlay when clicking outside the sidebar\r\n  overlay.addEventListener('click', function() {\r\n      sidebar.classList.remove('active');\r\n      overlay.classList.remove('active');\r\n      toggleButton.style.display = 'block'; // Show the toggle button again\r\n  });\r\n</script>\r\n\r\n</body>\r\n</html>\r\n
